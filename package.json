{
  "name": "react-webpack2",
  "version": "1.0.0",
  "private": true,
  "description": "React + Redux + Router starter kit with middlewares",
  "main": "index.js",
  "scripts": {
    "start": "bnr start",
    "storybook": "bnr storybook"
  },
  "moduleRoots": [
    "src"
  ],
  "betterScripts": {
    "start": {
      "command": "concurrently --kill-others \"bnr dev:watch\" \"bnr dev:start\""
    },
    "dev:start": {
      "command": "babel-node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "HOST": "127.0.0.1",
        "SSR_PORT": 3000,
        "HMR_PORT": 3001
      }
    },
    "dev:watch": {
      "command": "node ./webpack/dev.server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "HOST": "127.0.0.1",
        "SSR_PORT": 3000,
        "HMR_PORT": 3001
      }
    },
    "storybook": {
      "command": "start-storybook -p 9001 -s ./static --dont-track",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "react",
    "redux",
    "react-router"
  ],
  "author": "Vladislav Taiurskyi",
  "license": "ISC",
  "dependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "concurrently": "^3.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.1",
    "react": "^15.4.2",
    "react-a11y-alt": "romeovs/react-a11y",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "recompose": "^0.22.0",
    "redbox-react": "^1.3.3",
    "redial": "^0.5.0",
    "redux": "^3.6.0",
    "redux-connect": "^5.0.0",
    "redux-saga": "^0.14.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "rucksack-css": "^0.9.1",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.2",
    "superagent": "^3.4.3",
    "webpack-isomorphic-tools": "^2.6.6"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.23.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "beta",
    "file-loader": "^0.10.0",
    "flow-bin": "^0.39.0",
    "happypack": "^3.0.3",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^18.1.0",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.0",
    "postcss-calc": "^5.3.1",
