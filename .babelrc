{
  "presets": ["react", "es2015", "stage-3"],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-object-rest-spread"
    ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
